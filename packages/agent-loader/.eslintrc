{
    "parserOptions": {
      "globalReturn": true,
      "sourceType": "module",
      "ecmaVersion": 2020
    },
    "extends": "standard",
    "env": {
      "browser": true,
      "node": true
    },
    "globals": {
      "NREUM": true,
      "newrelic": true,
      "__nr_require": true,
      "attachEvent": true
    },
    "rules": {
      "space-before-function-paren": ["off"],
      "no-extend-native": 0,
      "no-wrap-func": 0,
      "camelcase": 0,
      "no-warning-comments": ["error"],
      "no-restricted-syntax": ["warn", {
        "selector": "ExportDefaultDeclaration",
        "message": "Prefer named exports"
      }],
    }
  }